entity Person{
	name String,
    surname String,
    gender Gender
}
enum Gender{
MALE , FEMALE
}

entity Car{
 model  String,
 color  String
}

entity Subscription{
	subDate LocalDate,
    person_id Long
}
relationship OneToMany{
	Person{cars} to Car
}

paginate Person with pagination
dto * with mapstruct
service all with serviceImpl 

microservice * with person except Subscription
microservice Subscription with subscription
 
